#!/bin/bash

#SBATCH --partition=gpu_titanrtx_shared
#SBATCH --gres=gpu:1
#SBATCH --job-name=recommender_lastfm
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=1
#SBATCH --time=00:15:00
#SBATCH --mem=6G
#SBATCH --output=/home/%u/job_logs/%x_%A_%u.out

module purge
source ~/scripts/load_modules.sh

source activate FACT

code_dir="~/FACT/src"

python $code_dir/recommender.py